{
  "compileOnSave": false,
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "alwaysStrict": true,
    "strictPropertyInitialization": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "isolatedModules": true,
    "strictBindCallApply": false,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "module": "ES6",
    "moduleResolution": "node",
    "target": "ESNext",
    "jsx": "react-jsx",
    "types": [
      "node",
      "jquery",
      "tampermonkey"
    ]
  }
}